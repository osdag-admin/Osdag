   \DeclareUTFcharacter[#1]{x2132}{\Finv}%
   \DeclareUTFcharacter[#1]{x2136}{\beth}%
   \DeclareUTFcharacter[#1]{x2138}{\daleth}%
   \DeclareUTFcharacter[#1]{x2141}{\Game}%
  }%
