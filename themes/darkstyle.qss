/* QComboBox --------------------------------------------------------------

https://doc.qt.io/qt-5/stylesheet-examples.html#customizing-qcombobox

--------------------------------------------------------------------------- */
QComboBox {

    background-color: #1D282F;
    border: 1px solid #37495b;
    border-radius: 0;
    /*padding: 3px;*/
    font: 16px "Arial";
    min-height: 25px;
}

QComboBox:hover{
    border: 1px solid #148CD2;
}

QComboBox QAbstractItemView {
    /*background-color: #242424;*/
    color: #bbbbbb;
    font: 16px "Arial";
    border-radius: 0;
    border: 1px solid #37495b;
    outline: 0;
  	selection-background-color: #148CD2;
  	selection-color:black ;
    margin-left: 0px;
    padding-left: 0px;

}

QComboBox QAbstractItemView::item {
    padding-top: 4px;
    padding-bottom: 4px;
    background-color: #1D282F;
    /* Add horizontal separators between items */
    /* border-bottom: 1px solid #343434; */
    text-align:left;
}

QComboBox QAbstractItemView::item::hover,
QComboBox QAbstractItemView::item::focus {
    color:red;

}


QComboBox::drop-down {
    border-top-right-radius: 3px;
    border-bottom-right-radius: 3px;
}

QComboBox::down-arrow,
QComboBox::down-arrow:focus {
    image: url("themes/rc/listview_down.png");
}

QComboBox::down-arrow:on {
    image: url("themes/rc/listview_up.png");
}

/* QSlider ----------------------------------------------------------------

https://doc.qt.io/qt-5/stylesheet-examples.html#customizing-qslider

--------------------------------------------------------------------------- */
QSlider:disabled {
  background: rgba(19, 27, 33, 1);
}

QSlider:focus {
  border: none;
}

QSlider::groove:horizontal {
  background: #1D282F;
  border: 1px solid #1D282F;
  height: 4px;
  margin: 0px;
  border-radius: 4px;
}

QSlider::groove:vertical {
  background: #1D282F;
  border: 1px solid #1D282F;
  width: 4px;
  margin: 0px;
  border-radius: 4px;
}

QSlider::add-page:vertical {
  background: #1464A0;
  border: 1px solid #1D282F;
  width: 4px;
  margin: 0px;
  border-radius: 4px;
}

QSlider::add-page:vertical :disabled {
  background: #14506E;
}

QSlider::sub-page:horizontal {
  background: #1464A0;
  border: 1px solid #1D282F;
  height: 4px;
  margin: 0px;
  border-radius: 4px;
}

QSlider::sub-page:horizontal:disabled {
  background: #14506E;
}

QSlider::handle:horizontal {
  background: #787878;
  border: 1px solid #1D282F;
  width: 8px;
  height: 8px;
  margin: -8px 0px;
  border-radius: 4px;
}

QSlider::handle:horizontal:hover {
  background: #148CD2;
  border: 1px solid #148CD2;
}

QSlider::handle:vertical {
  background: #787878;
  border: 1px solid #1D282F;
  width: 8px;
  height: 8px;
  margin: 0 -8px;
  border-radius: 4px;
}

QSlider::handle:vertical:hover {
  background: #148CD2;
  border: 1px solid #148CD2;
}

/* QLineEdit --------------------------------------------------------------

https://doc.qt.io/qt-5/stylesheet-examples.html#customizing-qlineedit

--------------------------------------------------------------------------- */
QLineEdit {
  background-color: #1D282F;
  color: #F0F0F0;
  border: 1px solid #37495b;
   border-radius: 0;
}

QLineEdit:disabled {
  background-color: rgba(19, 27, 33, 1);
  color: #787878;
}

QLineEdit:hover {
  border: 1px solid #148CD2;
  color: #F0F0F0;
}

QLineEdit:selected {
  background: #1464A0;
  color: #1D282F;
}

