QSplitter::separator:hover {
  background-color: #787878;
}

QSplitter::separator:horizontal {
  width: 5px;
  image: url("themes/rc/line_vertical.png");
}

QSplitter::separator:vertical {
  height: 5px;
  image: url("themes/rc/line_horizontal.png");
}

/* QDateEdit --------------------------------------------------------------

--------------------------------------------------------------------------- */
QDateEdit {
