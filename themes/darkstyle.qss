}

/* QHeaderView ------------------------------------------------------------

https://doc.qt.io/qt-5/stylesheet-examples.html#customizing-qheaderview

--------------------------------------------------------------------------- */
QHeaderView {
  background-color: #1D282F;
  border: 0px transparent #1D282F;
  padding: 0px;
  margin: 0px;
  border-radius: 0px;
}

QHeaderView:disabled {
  background-color: #1D282F;
  border: 1px transparent #1D282F;
  padding: 2px;
}

QHeaderView::section {
  background-color: #1D282F;
  color: #F0F0F0;
  padding: 2px;
  border-radius: 0px;
  text-align: left;
}

QHeaderView::section:checked {
  color: #F0F0F0;
  background-color: #1464A0;
}

QHeaderView::section:checked:disabled {
  color: #787878;
  background-color: #14506E;
}

QHeaderView::section::horizontal {
  border-left: 1px solid rgba(19, 27, 33, 1);
}

QHeaderView::section::horizontal::first, QHeaderView::section::horizontal::only-one {
  border-left: 1px solid #1D282F;
}

QHeaderView::section::horizontal:disabled {
  color: #787878;
}

QHeaderView::section::vertical {
  border-top: 1px solid rgba(19, 27, 33, 1);
}

QHeaderView::section::vertical::first, QHeaderView::section::vertical::only-one {
  border-top: 1px solid #1D282F;
}

QHeaderView::section::vertical:disabled {
  color: #787878;
}

QHeaderView::down-arrow {
  /* Those settings (border/width/height/background-color) solve bug */
  /* transparent arrow background and size */
  background-color: #1D282F;
  height: 12px;
  width: 12px;
  image: url("themes/rc/arrow_down.png");
}

QHeaderView::up-arrow {
  background-color: #1D282F;
  height: 12px;
  width: 12px;
  image: url("themes/rc/arrow_up.png");
}

/* QToolBox --------------------------------------------------------------

https://doc.qt.io/qt-5/stylesheet-examples.html#customizing-qtoolbox

--------------------------------------------------------------------------- */
QToolBox {
  padding: 0px;
  border: 0px;
  border: 1px solid #1D282F;
}

QToolBox::selected {
  padding: 0px;
  border: 2px solid #1464A0;
}

QToolBox::tab {
  background-color: rgba(19, 27, 33, 1);
  border: 1px solid #1D282F;
  color: #F0F0F0;
  border-top-left-radius: 4px;
  border-top-right-radius: 4px;
}

QToolBox::tab:disabled {
  color: #787878;
}

QToolBox::tab:selected {
  background-color: #505F69;
  border-bottom: 2px solid #1464A0;
}

QToolBox::tab:selected:disabled {
  background-color: #1D282F;
  border-bottom: 2px solid #14506E;
}

QToolBox::tab:!selected {
  background-color: #1D282F;
  border-bottom: 2px solid #1D282F;
}

QToolBox::tab:!selected:disabled {
  background-color: rgba(19, 27, 33, 1);
}

QToolBox::tab:hover {
  border-color: #148CD2;
  border-bottom: 2px solid #148CD2;
}

QToolBox QScrollArea QWidget QWidget {
  padding: 0px;
  border: 0px;
  background-color: rgba(19, 27, 33, 1);
}

/* QFrame -----------------------------------------------------------------

https://doc.qt.io/qt-5/stylesheet-examples.html#customizing-qframe

--------------------------------------------------------------------------- */
.QFrame {
  border-radius: 4px;
  border: 1px solid #1D282F;
  background-color: rgba(19, 27, 33, 1);
}

.QFrame[frameShape="0"] {
  border-radius: 4px;
  border: 1px transparent #1D282F;
}

.QFrame[height="3"], .QFrame[width="3"] {
  background-color: rgba(19, 27, 33, 1);
}

/* QSplitter --------------------------------------------------------------

https://doc.qt.io/qt-5/stylesheet-examples.html#customizing-qsplitter

--------------------------------------------------------------------------- */
QSplitter {
  background-color: #1D282F;
  spacing: 0px;
  padding: 0px;
  margin: 0px;
}

QSplitter::separator {
  background-color: #1D282F;
  border: 0px solid rgba(19, 27, 33, 1);
  spacing: 0px;
  padding: 1px;
  margin: 0px;
